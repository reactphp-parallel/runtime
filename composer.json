{
    "name": "react-parallel/runtime",
    "description": "\ud83d\udca8 Convinence wrapper around ext-parallel Runtime for and ReactPHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com",
            "homepage": "http://wyrihaximus.net/"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-parallel": "*",
        "react-parallel/event-loop": "^1.0",
        "react/event-loop": "^1.1",
        "react/promise": "^2.7",
        "wyrihaximus/constants": "^1.4.3"
    },
    "require-dev": {
        "wyrihaximus/async-test-utilities": "^2.2",
        "wyrihaximus/ticking-promise": "^1.6"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ReactParallel\\Runtime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReactParallel\\Tests\\Runtime\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
